CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE PUBLIC.USERS(userEmail VARCHAR(60) PRIMARY KEY NOT NULL, userName VARCHAR(20),userPassword VARCHAR(20) NOT NULL)
CREATE MEMORY TABLE PUBLIC.Recipes(recipeID INTEGER PRIMARY KEY NOT NULL, name VARCHAR(60), servings INTEGER, prepTime INTEGER, cookTime INTEGER, picture VARCHAR(100), userID VARCHAR(60))
CREATE MEMORY TABLE PUBLIC.Ingredients(ingredient VARCHAR(60) NOT NULL, recipeID INTEGER NOT NULL, CONSTRAINT CONSTRAINT1 PRIMARY KEY(ingredient,recipeID), CONSTRAINT CONSTRAINT2 FOREIGN KEY(recipeID) REFERENCES Recipes(recipeID))
CREATE MEMORY TABLE PUBLIC.Instructions(instruction VARCHAR(300) NOT NULL, recipeID INTEGER NOT NULL, instructionNum INTEGER NOT NULL, CONSTRAINT CONSTRAINT1 PRIMARY KEY(instruction,recipeID), CONSTRAINT CONSTRAINT2 FOREIGN KEY(recipeID) REFERENCES Recipes(recipeID))
